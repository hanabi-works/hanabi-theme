  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5C6E89",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#82FA82"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#FFB65E"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage"
      ],
      "settings": {
        "foreground": "#FF6FA0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#5CFFF0"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#6F90FF"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#A3B9FF"
      }
    },
    {
      "scope": [
        "constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#FFF066"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FAF9F9",
        "background": "#FB6583"
      }
    },
    {
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#FFE74A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#92FFF2",
        "background": "#302D43"
      }
    },
    {
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FC6B74"
      }
    },
    {
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#87F879"
      }
    },
    {
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#FFE74A"
      }
    },
    {
      "scope": [
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#6F90FF"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#5C6E89"
      }
    },
    {
      "scope": [
        "text.html.basic entity.other.attribute-name",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#F299BD"
      }
    }
  ],
  "semanticTokenColors": {
    "comment": "#5C6E89",
    "string": "#82FA82",
    "number": "#FFB65E",
    "keyword": "#FF6FA0",
    "function": "#5CFFF0",
    "variable": "#FCFCFA",
    "type": "#6F90FF",
    "class": "#A3B9FF",
    "property": "#92FFF2",
    "enumMember": "#FFF066",
    "operator": "#5C6E89",
    "namespace": "#F299BD"
  }
}
